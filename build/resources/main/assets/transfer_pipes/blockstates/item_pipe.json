{
  "multipart": [
    {
      "apply": {
        "model": "transfer_pipes:block/item_pipe_core"
      }
    },
    {
      "when": {
        "north": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side"
      }
    },
    {
      "when": {
        "south": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side",
        "x": 270
      }
    },
    {
      "when": {
        "down": "pipe|input|output"
      },
      "apply": {
        "model": "transfer_pipes:block/item_pipe_side",
        "x": 90
      }
    }
  ]
}